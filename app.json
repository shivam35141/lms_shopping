{
  "expo": {
    "name": "LMS Shopping",
    "slug": "lms-shopping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/welcome.png",
      "resizeMode": "contain",
      "backgroundColor": "#00b4d8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "host.lms.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "host.lms.shopping"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
